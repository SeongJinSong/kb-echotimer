{
  "name": "kb-echotimer-frontend",
  "version": "1.0.0",
  "description": "KB EchoTimer Frontend - React TypeScript Client",
  "type": "module",
  "scripts": {
    "dev": "npm run install-if-needed && vite",
    "install-if-needed": "node -e \"if (!require('fs').existsSync('node_modules')) { console.log('Installing dependencies...'); require('child_process').execSync('npm install', {stdio: 'inherit'}); }\"",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "keywords": ["timer", "websocket", "react", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@stomp/stompjs": "^7.2.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/sockjs-client": "^1.5.4",
    "@vitejs/plugin-react": "^5.0.3",
    "axios": "^1.12.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sockjs-client": "^1.6.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  }
}
